#using of colspan and rowspan in table
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table border="2">
        <tr>
            <td>colum1</td>
            <td>colum2</td>
            <td>colum3</td>
        </tr>
        <tr>
            <td rowspan=""2>row 1 cell1</td>
            <td>row 1 cell2</td>
            <td>row 1 cell3</td>
        </tr>
        <tr>
            <td>row 2 cell2</td>
            <td>row 2 cell3</td>
        </tr>
        <tr>
          <td colspan="3">Row 3 cell1</td>  
        </tr>
    </table>
</body>
</html>

